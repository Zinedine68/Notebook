FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         pimpleFoam;

startFrom           startTime;

startTime           105.4;

stopAt              endTime;

endTime             118.9; // Tc = 9e-3

deltaT              0.00005;

writeControl        runTime;

writeInterval       0.045;

purgeWrite          0;

writeFormat         ascii;

writePrecision      8;

writeCompression    off;

timeFormat          general;

timePrecision       8;

runTimeModifiable   true;


